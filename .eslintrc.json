{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jasmine": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prefer-arrow-callback": "off",
    "max-len": ["error", { "code": 100, "ignoreTemplateLiterals": true }]
  }
}
